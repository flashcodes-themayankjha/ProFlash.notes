{
  "expo": {
    "name": "ProFlash",
    "slug": "ProFlash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/android.png",
    "scheme": "proflash",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ProFlash",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false },
        "splash": {
    "image": "./assets/images/splash-dark.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.ProFlash"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash-dark.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "45b95981-240d-4269-8e73-3f6781275e93"
      }
    },
    "owner": "mayankjha07"
  }
}
